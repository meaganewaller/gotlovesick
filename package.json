{
  "name": "secretgardens",
  "version": "1.0.0",
  "license": "ISC",
  "description": "The frontend for secret gardens built with NextJS",
  "private": true,
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint -c eslint.config.cjs ./",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.1.0",
    "@vercel/analytics": "1.4.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.3",
    "feed": "^4.2.2",
    "framer-motion": "^11.15.0",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "next": "^15.1.0",
    "prismjs": "^1.29.0",
    "react": "^19.0.0",
    "react-buzzfeed-quiz": "2.0.7",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "rehype-parse": "^9.0.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "schema-dts": "^1.1.2",
    "splitting": "1.1.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@next/eslint-plugin-next": "^15.1.2",
    "@svgr/webpack": "8.1.0",
    "@types/node": "22.10.5",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.0",
    "@types/splitting": "1.0.6",
    "autoprefixer": "10.4.20",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "postcss": "8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "typescript": "5.7.2"
  }
}
