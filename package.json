{
  "name": "gotlovesick",
  "version": "1.0.0",
  "license": "ISC",
  "description": "The frontend for gotlovesick.com built with NextJS",
  "private": true,
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint -c eslint.config.cjs ./",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix"
  },
  "dependencies": {
    "@next/bundle-analyzer": "15.1.6",
    "@vercel/analytics": "1.5.0",
    "axios": "1.7.9",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "dompurify": "3.2.4",
    "feed": "4.2.2",
    "framer-motion": "12.4.2",
    "graphql": "16.10.0",
    "lodash": "4.17.21",
    "masonic": "4.0.1",
    "masonry-layout": "4.2.2",
    "next": "15.1.6",
    "next-seo": "6.6.0",
    "nprogress": "0.2.0",
    "postcss": "8.5.2",
    "prismjs": "1.29.0",
    "react": "19.0.0",
    "react-cookie": "7.2.2",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "react-resize-detector": "12.0.2",
    "react-scroll": "1.9.2",
    "rehype-parse": "9.0.1",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "10.0.1",
    "schema-dts": "1.1.2",
    "splitting": "1.1.0",
    "swr": "2.3.2",
    "uuid": "11.0.5",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@next/eslint-plugin-next": "15.1.6",
    "@parcel/watcher": "2.5.1",
    "@svgr/webpack": "8.1.0",
    "@types/node": "22.13.1",
    "@types/prismjs": "1.26.5",
    "@types/react": "19.0.8",
    "@types/react-scroll": "1.8.10",
    "@types/splitting": "1.0.6",
    "@wordpress/eslint-plugin": "22.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.20.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "10.0.1",
    "eslint-webpack-plugin": "4.2.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.5.0",
    "rimraf": "6.0.1",
    "typescript": "5.7.3"
  },
  "overrides": {
    "react-scroll": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
