{
  "name": "secretgardens",
  "version": "1.0.0",
  "license": "ISC",
  "description": "The frontend for secret gardens built with NextJS",
  "private": true,
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "clean": "rimraf .next out node_modules/.cache",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm start",
    "export": "npm run build && next export",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint -c eslint.config.cjs ./",
    "format": "prettier --write 'src/**/*.{json,md,css,js,jsx,ts,tsx}' && npm run lint -- --fix"
  },
  "dependencies": {
    "@next/bundle-analyzer": "15.1.4",
    "@vercel/analytics": "1.4.1",
    "date-fns": "4.1.0",
    "dompurify": "3.2.3",
    "feed": "4.2.2",
    "framer-motion": "11.18.0",
    "graphql": "16.10.0",
    "lodash": "4.17.21",
    "next": "15.1.4",
    "prismjs": "1.29.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "react-resize-detector": "12.0.2",
    "rehype-parse": "9.0.1",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "10.0.1",
    "schema-dts": "1.1.2",
    "splitting": "1.1.0",
    "swr": "2.3.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.5",
    "@next/eslint-plugin-next": "15.1.4",
    "@parcel/watcher": "2.5.0",
    "@svgr/webpack": "8.1.0",
    "@types/node": "22.10.7",
    "@types/prismjs": "1.26.5",
    "@types/react": "19.0.7",
    "@types/splitting": "1.0.6",
    "autoprefixer": "10.4.20",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "10.0.1",
    "husky": "9.1.7",
    "lint-staged": "15.4.1",
    "postcss": "8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "typescript": "5.7.3"
  }
}
